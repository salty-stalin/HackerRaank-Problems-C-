#include <iostream>
#include <vector>


int main ()
{
std::vector<int> score{0,0};
std::vector<int> alice = { 17, 28, 30 };
std::vector<int> bob = { 99, 16, 8 };
int i;
int al_score = 0;
int bob_score= 0;
for (i = 0 ; i <= 2 ; i++){
    if (alice[i]>bob[i]){
        al_score=al_score+1;
    }
    else if 
        (alice[i]<bob[i]){
            bob_score=bob_score+1;
        }
}
       std::cout<< al_score << " "<< bob_score << "\n";
       return 0;

}
